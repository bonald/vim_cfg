	<COMMAND name="update" help="Update system"/>
	<COMMAND name="update port" help="Phy port">
		<PARAM name="information" help="Information" ptype="SUBCOMMAND" mode="subcommand"/>
		<PARAM name="port-connection" help="Port-connection" ptype="SUBCOMMAND" mode="subcommand" />
		<PARAM name="port_connection" help="Must be / or -" ptype="PORT-CONNECTION" />
		<PARAM name="sub-port-connection" help="Sub-port-connection" ptype="SUBCOMMAND" mode="subcommand" />
		<PARAM name="sub_port_connection" help="Must be / or - or :" ptype="SUBPORT-CONNECTION" />

		<PARAM name="name1" help="Short name of 100M port (Example:ZMeg)" ptype="STRING" optional="true" default="none"/>
		<PARAM name="fullname1" test='"${name1}"!="none"' help="Full name of 100M port (Example:ZMegaEthernet)" ptype="STRING" />
		<PARAM name="name2"     test='"${name1}"!="none"' help="Short name of 1G port (Example:Gig)" ptype="STRING" />
		<PARAM name="fullname2" test='"${name1}"!="none"' help="Full name of 1G port (Example:GigabitEthernet)" ptype="STRING" />
		<PARAM name="name3"     test='"${name1}"!="none"' help="Short name of 10G port (Example:XGig)" ptype="STRING" />
		<PARAM name="fullname3" test='"${name1}"!="none"' help="Full name of 10G port (Example:XGigabitEthernet)" ptype="STRING" />
		<PARAM name="name4"     test='"${name1}"!="none"' help="Short name of 40G port (Example:QGig)" ptype="STRING" />
		<PARAM name="fullname4" test='"${name1}"!="none"' help="Full name of 40G port (Example:QGigabitEthernet)" ptype="STRING" />
		<PARAM name="name5"     test='"${name1}"!="none"' help="Short name of 100G port (Example:ZGig)" ptype="STRING" />
		<PARAM name="fullname5" test='"${name1}"!="none"' help="Full name of 100G port (Example:ZGigabitEthernet)" ptype="STRING" />

		<ACTION>
			swap_port="`fnconvert -c encoding -m '${port_connection}'`"
			swap_sub_port="`fnconvert -c encoding -m '${sub_port_connection}'`"

			if [ "${name1}" == "none" ]; then
				cdbctl update/cdb/sys/update_port_connection/$swap_port/$swap_sub_port
			else
				cdbctl update/cdb/sys/update_port_connection/$swap_port/$swap_sub_port/${name1}/${fullname1}/${name2}/${fullname2}/${name3}/${fullname3}/${name4}/${fullname4}/${name5}/${fullname5}/
			fi
		</ACTION>

		<CTCDESCRIPTION>

		</CTCDESCRIPTION>
		<CTCUSAGE>

		</CTCUSAGE>
		<CTCDEFAULT>

		</CTCDEFAULT>
		<CTCEXAMPLE>
		</CTCEXAMPLE>
		<CTCRCMD>
		</CTCRCMD>
	</COMMAND>

